# Test Environment Configuration
NODE_ENV=test

# Treblle Configuration
TREBLLE_API_KEY=your_treblle_api_key_here
TREBLLE_PROJECT_ID=your_treblle_project_id_here

# Database Configuration (Use separate test database)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=treblle_api_test
DB_USER=treblle_test_user
DB_PASSWORD=test_password
DB_POOL_MAX=5
DB_IDLE_TIMEOUT=10000
DB_CONNECTION_TIMEOUT=5000

# JWT Configuration
JWT_SECRET=test-jwt-secret-key-for-testing-only
JWT_EXPIRES_IN=1h

# Server Configuration
PORT=0

# Ollama Configuration (Mock in tests)
OLLAMA_API_URL=http://localhost:11434
OLLAMA_MODEL=moondream

# Rate Limiting (Relaxed for tests)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=1000

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

